    function numberOnly(event, w) {

        var key = window.event ? event.keyCode : event.which;
        if (w == 'decimal') {

            if ((key > 45 && key < 58 && key != 47) || (key == 8 || key == 0))

                return true;
        }
        else if (w == 'letters') {
            if ((key > 64 & key < 91) || (key > 96 & key < 122) || (key == 8 || key == 0) || key == 32)
                return true;
        }
        else {
